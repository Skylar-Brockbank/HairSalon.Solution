@model HairSalon.Models.Client
<h1>Enter New Client Name</h1>
@using (Html.BeginForm())
{
    @Html.LabelFor(model => model.Name)
    @Html.TextBoxFor(model => model.Name)
    @Html.DropDownListFor(model => model.StylistId, (List<SelectListItem>) ViewBag.StylistDropdown)
    <input type="submit" value="Add New Client" />
}